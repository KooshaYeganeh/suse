psad(){



	mkdir /tmp/psad
	cd /tmp/psad
	wget http://www.cipherdyne.org/psad/download/psad-2.2-1.x86_64.rpm
	rpm -ivh psad-2.2-1.x86_64.rpm 
	cd ..
	rm -rf psad

	if [ -d "/etc/psad" ];then
		echo "psad Installed Successfully [ OK ]"
	else
		echo "Error on psad installation"
	fi

}


